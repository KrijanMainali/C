#include <iostream>
using namespace std;

int i , j = 0;
int arr1[3],arr2[3];
void cartesian(int arr1[3] ,int arr2[3]){
    for (i = 0 ; i<3 ; i++){
        for ( j=0 ; j<3 ; j++){
            cout<<"("<<arr1[i]<<","<<arr2[j]<<")"<<",";
        }
    }
}
int main()
{
    cout<<"Enter the first set :"<<endl;
    for (i = 0; i<3 ; i++){
        cin>>arr1[i];
    }
    cout<<"Enter the second set :"<<endl;
    for (j = 0; j<3 ; j++){
        cin>>arr2[j];
    }
    cartesian(arr1 , arr2);
}
